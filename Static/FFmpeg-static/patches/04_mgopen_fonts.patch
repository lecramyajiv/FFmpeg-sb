--- a/conf.d/65-nonlatin.conf
+++ b/conf.d/65-nonlatin.conf
@@ -110,7 +110,9 @@
 			<family>PingFang HK</family> <!-- han (zh-hk) - macOS -->
 			<family>Hiragino Sans CNS</family> <!-- han (zh-cn) - macOS -->
