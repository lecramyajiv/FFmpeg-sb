diff --git a/conf.d/40-nonlatin.conf b/conf.d/40-nonlatin.conf
index f8d96ce..c99534b 100644
--- a/conf.d/40-nonlatin.conf
+++ b/conf.d/40-nonlatin.conf
@@ -157,10 +157,18 @@
